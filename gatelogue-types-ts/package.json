  "types": "./dist/lib.d.ts",
  "exports": {
    "import": "./dist/lib.js",
    "require": "./dist/cjs/lib.js"
  },
  "files": [
    "dist",
    "tsconfig.json"
  ],
  "devDependencies": {
    "@eslint/js": "9.39.1",
